(
    (b none)
    (P none)
    (uz none)

    (b sigma.buoyant)
    (P sigma.buoyant)
    (uz sigma.buoyant)
    (sigma.buoyant none)

    (b sigma.stable)
    (P sigma.stable)
    (uz sigma.stable)
    (sigma.stable none)
);

