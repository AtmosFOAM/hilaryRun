FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          transferProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

noTransfers false;
cloudRadiusMax     cloudRadiusMax [0 1 0 0 0] 5000;
cloudRadiusMin     cloudRadiusMin [0 1 0 0 0] 5000;
dragCoeff       1;
Ksigma Ksigma [0 2 -1 0 0] 0; //1e3;
minSigma  0;
// Transfer between partitions based on horizontal divergence
wTransfer false;
divTransfer 0;
// Transfer between partitions based on laplacian(b)
buoyancyTransfer false;
transferAnomalies true;
bTransferDiffusivity bTransferDiffusivity [0 2 -1 0 0] 0;
transferAnomaliesLaplacian false;
transferAnomaliesb0 false;

uniformSigma true;

