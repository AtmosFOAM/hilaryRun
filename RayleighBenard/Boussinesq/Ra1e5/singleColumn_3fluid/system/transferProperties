FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          transferProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

fluids 3 (stable up down);
transferType dwdzTransfer;
transferScale 1;
minSigma  3(0 0 0);

wTransferScale 1;
bTransferScale 0;

Ksigma Ksigma [0 2 -1 0 0] 100;
gamma    gamma    [0 2 -1 0 0] 1371; //(gammaHat = 1)

cloudRadius     cloudRadius [0 1 0 0 0] 0;
dragCoeff 0;

