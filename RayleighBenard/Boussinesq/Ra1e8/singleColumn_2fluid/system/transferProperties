FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          transferProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

fluids 2 (down up);
transferType dwdzTransfer;
transferScale 1;
minSigma  3(0 0 0);

wTransferScale 1;
bTransferScale 0;

Ksigma Ksigma [0 2 -1 0 0] 0;
// gamma = gammaHat0 nu Ra^.25 Pr^-.25
// Ra = 1e8 Pr = 0.707 nu = 2.2357303057
gamma    gamma    [0 2 -1 0 0] 243.8173239723; //(gammaHat = 1)


cloudRadius     cloudRadius [0 1 0 0 0] 0;
dragCoeff 0;

