FoamFile
{
    version         2.3;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/2c";
projection2D XZ;

FieldsToPlot
(
    [massTransfer.buoyant.stable rawValues 0 1.2 0.1 "$GMTU/colours/white_grey.cpt"]
);

separateLegend true;
region "0/10/-0.1/1.1";
plotBoundaryDots false;
annotate "minMax 3 1.3";

