FoamFile
{
    version         2.3;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X30.3c/15c";
projection2D XZ;
patch front;

FieldsToPlot
(
    [thetap filledContours -0.525 0.525 0.05 "$GMTU/colours/red_white_blue.cpt"]
    [Co contours 1 101 50 "black,2_5:0"]
    [Co contours 1.5 10 0.5 "black"]
);

separateLegend true;
