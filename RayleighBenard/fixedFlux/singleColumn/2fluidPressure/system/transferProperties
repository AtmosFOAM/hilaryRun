transferType divTransfer; // Could be divTransfer, pressureTransfer, special or noTransfer
                      // or diffusionOnly
wTransfer0 1.7;
bAnomalyMeanCoeff 1;

cloudRadius     cloudRadius [0 1 0 0 0] 0;
dragCoeff 0;

Ksigma Ksigma [0 2 -1 0 0] 0;
minSigma  2(0.01 0.01);

