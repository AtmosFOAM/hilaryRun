FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volVectorFieldValue U (0.5 0 0)
);

regions
(
    // Setting values inside a box
    boxToCell
    {
        box     (0.26 0 0) (0.76 1 1);
        fieldValues
        (
            volVectorFieldValue U (1 0 0)
        );
    }
);


