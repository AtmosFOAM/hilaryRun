FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

regions
(
    // A top hat distribution
    boxToCell
    {
        box (0.6 0 0) (0.8 1 1);
        fieldValues
        (
            volScalarFieldValue T 1
        );
    }
);

