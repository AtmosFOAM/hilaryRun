FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default Euler;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default             none;
    div((0.5*(phi+phi_0)),T)          MPDATA MPDATAtype;
}

laplacianSchemes
{
    default         none;
}

interpolationSchemes
{
    default        none;
}

snGradSchemes
{
    default         uncorrected;
}


