FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      velocityDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

timeVaryingWind true;
type deformationalNonDivergentGeodesic;
radius 1;
deformationScale 10;
endTime endTime [0 0 1 0 0] 5;
centre (0 0 0);
centre (0 0 0);
centre (0 0 0);
