FoamFile
{
    version         1.4;
    format          ascii;

    root            "/export/lime/data-04/sws02hs/OpenFOAM/sws02hs-1.4.1/run/shallowWater";
    case            "bucky4_30";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
projection "Q180/18c";
projection2D latLon;
plotBoundaryDots false;

FieldsToPlot
(
    [T filledContours 0 0.95 0.025 "$GMTU/colours/wh-bl-gr-ye-re.cpt"]
//    [anteDV   vectors 10.1 7 "black"]
    [Co colouredContours 0 15 1 "$GMTU/colours/white_black.cpt"]
);

// options
//region "0/360/-90/90";
boundaryMarks "a60/a60";
separateLegend true;
annotate "minMax 180 -105";
lonMin 0;

