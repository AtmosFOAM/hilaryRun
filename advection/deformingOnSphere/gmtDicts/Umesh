FoamFile
{
    version         1.4;
    format          ascii;

    root            "/export/lime/data-04/sws02hs/OpenFOAM/sws02hs-1.4.1/run/shallowWater";
    case            "bucky4_30";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
projection "G100/110/24c";
projection2D latLon;
plotBoundaryDots false;

FieldsToPlot
(
    [none mesh      "0.1p,grey"]
    [U vectors 9.5 7 "black"]
);

// options
region "-0/360/-90/90";
//boundaryMarks "a60/a60";
separateLegend true;
