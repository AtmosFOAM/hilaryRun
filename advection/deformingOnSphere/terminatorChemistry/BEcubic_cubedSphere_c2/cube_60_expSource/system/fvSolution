FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

tracerNames 2("X1" "X");

solvers
{
    "X|X1"
    {
        solver PBiCGStab;
        preconditioner   DILU;
        tolerance       1e-16;
        relTol          0;
        maxIter         10;
    }
}

