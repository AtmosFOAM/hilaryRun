FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

withDensity true;
tracerNames 3("rho" "T" "unity");

solvers
{

    "T|rho|unity"
    {
        solver          PBiCGStab;
        preconditioner  DILU;
        tolerance       1e-16;
        maxIter         3;
        relTol          0;
    }
}

