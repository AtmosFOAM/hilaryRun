FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      tracerDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type cone;
width 0.5;
centre (0.5 0 0.5);
maxTracer 1;
