FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      RASProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType RAS;

RAS
{
    RASModel        realizableKE;
    turbulence      on;
    printCoeffs     on;

    realizableKECoeffs
    {
        A0              4.0;
        C2              1.9;
        sigmak          1;
        sigmaEps        1.2;
    }

}



