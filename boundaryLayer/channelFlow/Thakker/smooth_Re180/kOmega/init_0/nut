FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.0335410615;

boundaryField
{
    ground
    {
        type            nutUWallFunction; //nutLowReWallFunction; //
        value           uniform 1;
        kappa           0.408; //0.415; //0.384; //0.41; //
        //Cmu             0.09; //0.06;
        //E               9.8;
    }
    centerLine
    {
        type            zeroGradient;
    }
    inlet
    {
        type            cyclic;
    }
    outlet
    {
        type            cyclic;
    }
    sides
    {
        type            empty;
    }
}


