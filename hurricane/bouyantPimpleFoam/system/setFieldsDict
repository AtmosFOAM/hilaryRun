FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

regions
(
    boxToCell
    {
        box (-20e3 -20e3 0) (20e3 20e3 1e3);


        fieldValues
        (
            volScalarFieldValue theta 300.1
        );
    }

);

