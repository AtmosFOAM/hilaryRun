FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      tracerFieldDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type gaussian;
width 100e3;
centre (1000e3 1000e3 0);
maxTracer 1e-3;


