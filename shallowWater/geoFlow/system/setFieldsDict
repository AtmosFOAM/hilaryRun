FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue h0 0
);

regions
(
    boxToCell
    {
        box (450e3 -50e3 0) (550e3 50e3 1000);

        fieldValues
        (
            volScalarFieldValue h0 5e3
        );
    }
);

