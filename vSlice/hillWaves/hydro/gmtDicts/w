FoamFile
{
    version         2.2;
    format          ascii;

    root            "";
    case            "";
    instance        "constant";
    local           "";

    class           dictionary;
    object          gmtDict;
}

// How to plot
// Compulsory inputs
projection "X18c/6c";
projection2D XZ;
boundaryPatch ground;
plotBoundaryDots false;

FieldsToPlot
(
    [Ufz contours 5e-4 1 5e-4 "black"]
    [Ufz contours -1 -5e-4 5e-4 "black,10_10:0"]
    [Ufz contours 0 200 100 "black,2_2:0"]
);

