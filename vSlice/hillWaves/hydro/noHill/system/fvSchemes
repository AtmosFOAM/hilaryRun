FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          fvSchemes;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

SIgravityWaves false;
implicitU      false;
stagger        false;

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default        Gauss linear;
    //linearUpwind   cellLimited Gauss linear 1;
}

divSchemes
{
    default none;
    div(phi,theta)    Gauss upwind;
    div(phi,U)        Gauss upwind;
    div(((rho*nuEff)*dev2(T(grad(U))))) Gauss linear uncorrected;
}

laplacianSchemes
{
    default         Gauss linear uncorrected;
}

interpolationSchemes
{
    default        linear;
}

snGradSchemes
{
    default         uncorrected;
}

fluxRequired
{
    default         no;
    Exner;
}


