FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      tracerDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


type    cosine;
width            1000;
centre           (500 500 2000);
maxTracer        1;
backgroundTracer 0;

